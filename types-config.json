  "source": {
    "include": ["."],
    "includePattern": ".(mjs)$"
  },
  "plugins": [ "./node_modules/@environment-safe/tsd-jsdoc/dist/plugin" ],
  "opts": {
      "template": "./node_modules/@environment-safe/tsd-jsdoc/dist"
  },
  "sourceType": "module"
}